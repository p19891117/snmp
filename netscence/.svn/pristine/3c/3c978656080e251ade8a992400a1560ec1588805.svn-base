@charset "utf-8";
/* CSS Document */
/* Essentials */
html,div,map,dt,isindex,form,header,aside,section,section,article,footer{display:block;font-family:"Microsoft Yahei ", "微软雅黑"; }
html,body{height:100%;margin:0;padding:0;font-family:"Microsoft Yahei ","Helvetica Neue",Helvetica,Arial,Verdana,sans-serif;background:#003557;font-size:12px}
ul li ol{list-style:none;margin:0;padding:0;outline:none;border:none}
ul, ol { list-style-type:none; }
li{list-style:none;}
.clearfix:before,.clearfix:after{content:'\0020';display:block;overflow:hidden;visibility:hidden;width:0;height:0;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.clear{clear:both;}
.spacer{height:20px}
a:link,a:visited{color:#0097ff;text-decoration:none}
a:hover{text-decoration:underline}
.txt_c{text-align:center;}
.txt_r{text-align:right;}
.mb30{margin-bottom:30px;}
.border{border:1px solid #ccc}
.bg_b{background:#000;}

/* Header */
header#header{height:90px;width:100%;background:#222222 url(../taskimages/header_bg.png) repeat-x}
header#header h2.section_title{float:left;margin:0;font-size:32px;font-weight:normal;line-height:90px;color:#fff;text-shadow:1 1px 0 #aaa; margin-left:5%}
header#header span{float:right; line-height:90px; font-size:14px; color:#fff; text-shadow:1 1px 0 #aaa; margin-right:5%;}

.top{background:#000; text-align:center;color:#aaa; line-height:50px; font-size:14px;}
.page{background:url(../taskimages/index.jpg) no-repeat center top; width:auto; height:700px;}
a.btn_jr{position:absolute; margin-left:300px; margin-top:360px; padding:10px 30px; background:#92ded2; border-radius:5px; -webkit-border-radius:5px; -moz-border-radius:5px; font-size:24px; color:#FFF;}
a.btn_jr:hover{text-decoration:none; color:#000;}
.dw{margin:10px 3% 0 3%;}
.dw1{border:#ccc 1px solid; margin-top:-30px; padding:20px; border-radius:3px;}
.dw2{text-align:center;margin:20px 0px;}
.dw2 input[type=radio]{margin-right:5px;}
.dw2 span{margin-top:2px; position:absolute;}


/* Main Content */
section#main{width:100%;margin:0 auto;min-height:500px;}
#main h3{color:#1F1F20;text-transform:uppercase;text-shadow:0 1px 0 #fff;font-size:16px;margin:9px 20px}
/* Modules */
.module{border:1px solid #9BA0AF;width:100%;margin:20px 3% 0 3%;margin-top:20px;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;background:#fff}
.module>div h3{display:block;width:90%;float:left}
.module>div{width:100%;background:#F1F1F4 url(../taskimages/secondary_bar.png) repeat-x;-webkit-border-top-left-radius:5px; -webkit-border-top-right-radius:5px;-moz-border-radius-topleft:5px; -moz-border-radius-topright:5px;border-top-left-radius:5px; border-top-right-radius:5px}
.module footer{height:32px;width:100%;border-top:1px solid #9CA1B0;background:#F1F1F4 url(../taskimages/module_footer_bg.png) repeat-x;-webkit-border-bottom-left-radius:5px; -webkit-border-bottom-right-radius:5px;-moz-border-radius-bottomleft:5px; -moz-border-radius-bottomright:5px;-webkit-border-bottom-left-radius:5px; -webkit-border-bottom-right-radius:5px}
.module_content{margin:10px 20px;color:#666}

/* Module Widths */
.width_full{width:95%}
/* Content Manager */
.tablesorter{width:90%;margin:-5px 0 0 0}
.tablesorter td{margin:0;padding:0;border-bottom:1px dotted #ccc}
.tablesorter td,.tablesorter th{border-right:1px dotted #ccc}
.tablesorter thead tr{height:34px;background:url(../taskimages/table_sorter_header.png) repeat-x;text-align:left;text-indent:10px;}
.tablesorter td{padding:10px}
.tablesorter input[type=image]{margin-right:10px}
/* Content Manager */
.tablesorter_b{width:100%;margin:-5px 0 0 0}
.tablesorter_b td{margin:0;padding:0;border-bottom:1px dotted #ccc}
.tablesorter_b td,.tablesorter th{border-right:1px dotted #ccc}
.tablesorter_b thead tr{height:34px;background:url(../taskimages/table_sorter_header.png) repeat-x;text-align:left;text-indent:10px;}
.tablesorter_b td{padding:10px}
.tablesorter_b input[type=image]{margin-right:10px}

/* box */
.box{position:absolute;width:400px;left:50%;height:auto;z-index:100;background-color:#fff;border:1px #8FA4F5 solid;padding:1px;}
.box h2{height:25px;font-size:14px;background-color:#3366cc;position:relative;padding-left:10px;line-height:25px;color:#fff;margin: 0;font-family:'Microsoft Yahei', '微软雅黑';}
.box h2 a{position:absolute;right:5px;font-size:12px;color:#fff;}
.box .mainlist{padding:10px 30px;}
.box .mainlist p{line-height:30px;font-family:'Microsoft Yahei', '微软雅黑'; text-align:center}
#TB_overlayBG{background-color:#666;position:absolute;z-index:99;left:0;top:0;display:none;width:100%;height:100%;opacity:0.5;filter: alpha(opacity=50);-moz-opacity: 0.5;}

.tab_container{ margin-top:15px;}
ul.tab{margin:0 auto;padding:0;list-style:none;height:24px; width:100%;}
ul.tab li{float:left;margin:0;padding:0;line-height:24px; height:24px;border-left:#ccc 1px solid;border-bottom:#ccc 2px solid;}

ul.tab2{margin:0 auto;padding:0;list-style:none;height:14px; width:100%;}
ul.tab2 li{float:left;margin:0;padding:0;line-height:14px; height:14px;border-left:#aaa 1px solid;border-bottom:#3399ff 2px solid;}
.point{border-radius:6px;background:#fcb713;height:8px;padding: 1px;width:8px;position: absolute;margin-top:-32px;z-index:333;}

ul.tab1{margin:0 auto;padding:0;list-style:none;height:24px; width:100%;}
ul.tab1 li{float:left;margin:0;padding:0;line-height:24px;}

ul.tabs{margin:-32px;list-style:none;height:24px; position:absolute; width:80%;}
ul.tabs li{float:left;margin:0;padding:0;line-height:10px;border-radius:10px;background:#fcb713; color:#fff; padding:1px 5px; text-align:center;}
ul.tabs li a{text-decoration:none;color:#999;display:block;height:10px}
ul.tabs li a:hover{color:#44474F}
ul.tabs li.active a{color:#fff; font-weight:bold;}

ul.tabsb{margin:-32px;list-style:none;height:24px; position:absolute; width:80%;}
ul.tabsb li{float:left;margin:0;padding:0;line-height:10px;border-radius:10px;background:#fff; color:#fff; padding:1px 5px; text-align:center;font-weight:bold;}
ul.tabsb li a{text-decoration:none;color:#999;display:block;padding:0 10px;height:10px}
ul.tabsb li a:hover{color:#44474F}
ul.tabsb li.active a{color:#fff; font-weight:bold;}

ul.tabsa{list-style:none;height:34px;}
ul.tabsa li{float:left;margin:0 30px;line-height:34px;border-radius:10px; background:#80cbff; padding:1px 5px; text-align:center; width:20%;margin-left:18%;}
ul.tabsa li a{text-decoration:none;color:#000;display:block;padding:0 10px;height:34px; font-size:14px;}
ul.tabsa li a:hover{color:#44474F}
ul.tabsa li.active a{color:#000; font-weight:bold;}

/* 设备实时采集信息展示 2015-01-08 */
ul.tab_sbss{list-style:none;height:34px;padding:0px; margin:0; background:#efefef;}
ul.tab_sbss li{float:left;line-height:34px;width:100px; background:#efefef; text-align:center; border-right:#ccc 1px solid; border-top:#ccc 1px solid;}
ul.tab_sbss li a{text-decoration:none;color:#000;display:block;height:34px; font-size:14px;}
ul.tab_sbss li a:hover{color:#44474F}
ul.tab_sbss li.active a{color:#3299e1; font-weight:bold; background:#f8f8f8;}
.sbss_jbxx{padding:40px 20px; background:#f8f8f8;}
.sbss_jbxx .sbss_jbxx1{width:49%; float:left; margin-bottom:40px;background:#fff;}
.sbss_jbxx .sbss_jbxx2{width:49%; float:left; margin-left:1%; margin-bottom:40px; background:#f8f8f8;}
.sbss_jbxx .sbss_jbxx3{width:50%; float:left;background:#f8f8f8;}
.sbss_jbxx .sbss_jbxx4{width:24%; float:left; margin-left:1%;background:#f8f8f8;}
.sbss_jbxx .sbss_jbxx5{width:24%; float:right; margin-left:1%;background:#f8f8f8;}
.sbss_wlxx{padding:40px 20px 0px 20px; background:#f8f8f8;}
.sbss_wlxx .sbss_wlxx1{width:49%; float:left; margin-bottom:40px;background:#fff;}
.sbss_tcpxx{padding:40px 20px 0px 20px; background:#f8f8f8;}
.sbss_tcpxx .sbss_tcpxx1{width:49%; float:left; margin-bottom:40px;background:#fff;}
.sbss_tcpxx .sbss_tcpxx2{float:left; margin-bottom:40px;background:#fff;}
.sbss_udpxx{padding:40px 20px 0px 20px; background:#f8f8f8;}
.sbss_udpxx .sbss_udpxx1{width:49%; float:left; margin-bottom:40px;background:#fff;}
.sbss_udpxx .sbss_udpxx2{float:left; margin-bottom:40px;background:#fff;}
.sbss_jcxx{padding:40px 20px 0px 20px; background:#f8f8f8;}
.sbss_jcxx .sbss_jcxx1{width:49%; float:left; margin-bottom:40px;background:#fff;}
.sbss_jcxx .sbss_jcxx2{width:24%; float:left; margin-bottom:40px;background:#fff;}
.sbss_jcxx .sbss_jcxx3{width:24%; float:left; margin-left:1%; margin-bottom:40px;background:#fff;}
