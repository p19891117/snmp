/* Messages */
.post_message{text-align:left;padding:35px 0 5px 0;}
input[type=text]{-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #bbb;height:26px; color:#ccc;-webkit-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;-moz-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;text-indent:10px;background-position:10px 6px}
input[type=text]:focus{outline:none;border:1px solid #77BACE;-webkit-box-shadow:inset 0 2px 2px #ccc,0 0 4px #80cbff;-moz-box-shadow:inset 0 2px 2px #ccc,0 0 4px #80cbff;box-shadow:inset 0 2px 2px #ccc,0 0 4px #80cbff;color:#666666}
select{-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #bbb;height:30px;width: 150px;color:#000;-webkit-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;-moz-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;text-indent:10px;background-position:10px 6px}
select:focus{outline:none;border:1px solid #77BACE;-webkit-box-shadow:inset 0 2px 2px #ccc,0 0 4px #80cbff;-moz-box-shadow:inset 0 2px 2px #ccc,0 0 4px #80cbff;box-shadow:inset 0 2px 2px #ccc,0 0 4px #80cbff;color:#666666}
.alt_btn{-moz-border-radius:3px;-webkit-border-radius:3px;background:#00aeef;border-radius:3px;color:#fff;display:inline-block;font:bold 14px/34px;padding:0 22px;position:relative;text-shadow:0 1px 0 #008bbf;border:none;height: 30px; margin-top:2px;}
.alt_btn:hover{opacity:0.7;text-decoration:none}


input[type=checkbox]{vertical-align:middle; width:12px; height:12px;}
input[type=submit].alt_btn1{background:#D0D1D4 url(../taskimages/btn_submit_2.png) repeat-x;border:1px solid#30B0C8;-webkit-box-shadow:0 1px 0 #fff;-moz-box-shadow:0 1px 0 #fff;box-shadow:0 1px 0 #fff;font-weight:bold;height:22px;-webkit-border-radius:2px;-moz-border-radius:2px;border-radius:2px;padding:0 10px;color:#003E49;text-shadow:0 1px 0 #6CDCF9;cursor:pointer}

input[type=submit].alt_btn1:hover{color:#001217}
input[type=submit].btn_post_message{background:#D0D1D4 url(../taskimages/post_message.png) no-repeat;display:block;width:37px;border:none;height:24px;cursor:pointer;text-indent:-5000px}
input[type=submit].btn_post_message:hover{background-position:0 -24px}

/* New/Edit Article Module */
fieldset{-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;background:#F6F6F6;border:1px solid #ccc;padding:1% 0%;margin:10px 0}
fieldset label{display:block;float:left;width:200px;height:25px;line-height:25px;text-shadow:0 1px 0 #fff;font-weight:bold;padding-left:10px;margin:-5px 0 5px 0;text-transform:uppercase}
fieldset input[type=text]{-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #BBBBBB;height:20px;color:#666666;-webkit-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;-moz-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;padding-left:10px;background-position:10px 6px;margin:0;display:block;float:left;width:96%;margin:0 10px}
fieldset input[type=text]:focus{outline:none;border:1px solid #77BACE;-webkit-box-shadow:inset 0 2px 2px #ccc,0 0 10px #ADDCE6;-moz-box-shadow:inset 0 2px 2px #ccc,0 0 10px #ADDCE6;box-shadow:inset 0 2px 2px #ccc,0 0 10px #ADDCE6}
fieldset select{width:96%;margin:0 10px;border:1px solid #bbb;height:20px;color:#666666}

textarea{-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #BBBBBB;color:#666666;-webkit-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;-moz-box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;box-shadow:inset 0 2px 2px #ccc,0 1px 0 #fff;padding-left:10px;background-position:10px 6px;display:block;float:left;width:87%;}
textarea:focus{outline:none;border:1px solid #77BACE;-webkit-box-shadow:inset 0 2px 2px #ccc,0 0 10px #ADDCE6;-moz-box-shadow:inset 0 2px 2px #ccc,0 0 10px #ADDCE6;box-shadow:inset 0 2px 2px #ccc,0 0 10px #ADDCE6}

.submit_link{float:right;margin-right:3%;padding:5px 0}
#main h4.alert_info{display:block;width:90%;margin:20px 5% 3% 5%;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;background:#ffedc1;border: 1px solid #ffc336;color: #082B33;padding: 10px;font-size: 14px;text-shadow:0 1px 0 #fff;}

#prbar{width:81%;background-color:#ddd;overflow:hidden;-moz-border-radius:5px;border-radius:5px; position:absolute;margin-top:-28px; z-index:111;}
doesnotexist:-o-prefocus, #prbar {border-radius:0px;}        
#prpos{background-color:#3399ff;width:0%;height:4px;transition: width 8s ease;-moz-transition: width 8s ease;-webkit-transition: width 8s ease;-o-transition: width 8s ease;-ms-transition: width 2s ease; }
   
.cjclpz{position:absolute; top:20%; left:38%; width:30%; background:#f1f1f1;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #ddd;-moz-box-shadow:1px 1px 1px 1px #fafafa inset;-webkit-box-shadow:1px 1px 1px 1px #fafafa inset;box-shadow:1px 1px 1px 1px #fafafa inset; padding:2%;}
.sjjd{position:absolute; top:36%; left:38%; width:30%; background:#f1f1f1;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #ddd;-moz-box-shadow:1px 1px 1px 1px #fafafa inset;-webkit-box-shadow:1px 1px 1px 1px #fafafa inset;box-shadow:1px 1px 1px 1px #fafafa inset; padding:1% 2%; line-height:30px; font-size:14px; z-index:999;}
.img_box{position:absolute; top:40%; left:25%; width:46%;margin:0 auto;background:#f1f1f1;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #ddd;-moz-box-shadow:1px 1px 1px 1px #fafafa inset;-webkit-box-shadow:1px 1px 1px 1px #fafafa inset;box-shadow:1px 1px 1px 1px #fafafa inset;padding:2%;}
.img_box img{float:left;}
.cjclpz_dw{position:absolute; margin-left:29.4%; margin-top:-12.4%;}
.cjclpz_box{position:absolute; top:50%; left:23%; width:50%; background:#f1f1f1;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;border:1px solid #ddd;-moz-box-shadow:1px 1px 1px 1px #fafafa inset;-webkit-box-shadow:1px 1px 1px 1px #fafafa inset;box-shadow:1px 1px 1px 1px #fafafa inset; padding:1% ;}
.cjclpz_box p{line-height:24px; font-size:14px;}





/*   2015-0310  改网页框架添加 css  */
.label,.badge{display:inline-block;padding:2px 4px;font-size:11.844px;font-weight:bold;line-height:14px;color:#fff;text-shadow:0 -1px 0 rgba(0,0,0,0.25);white-space:nowrap;vertical-align:baseline;background-color:#999}
.label{-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px}

/* Sidebar Navigation */
#sidebar{display:block;float:left;position:relative;width:220px;z-index:16; background:#003557;}
#sidebar > ul{list-style:none;margin:10px 0 0;padding:0;position:absolute;width:220px}
#sidebar > ul > li{display:block;position:relative;background:#003557;}
#sidebar > ul > li.active{box-shadow:0 2px 4px rgba(0,19,34,0.2) inset}
#sidebar > ul > li > a{padding:15px 0 15px 25px;display:block;color:#fff; font-size:14px;}
#sidebar > ul > li > a > i{margin-right:10px}
#sidebar > ul > li.active > a{background:url("../img/menu-active.png") no-repeat scroll right center transparent !important}
#sidebar > ul > li > a > .label{margin:0 20px 0 0;float:right;padding:3px 5px 2px;box-shadow:0 1px 2px rgba(0,0,0,0.5) inset,0 1px 0 rgba(255,255,255,0.2)}
#sidebar > ul ul{display:none;margin:0;padding:0;box-shadow:0 0 3px rgba(0,19,34,0.5) inset}
#sidebar > ul li.open ul{display:block}
#sidebar > ul ul li a{padding:10px 0 10px 40px;display:block;color:#6d9ec5;}
#sidebar > ul ul li:first-child a{border-top:0}
#sidebar > ul ul li:last-child a{border-bottom:0}
#sidebar > ul{border-top:1px solid #00223c;border-bottom:1px solid #084b7f}
#sidebar > ul > li{border-top:1px solid #084b7f;border-bottom:1px solid #00223c}
#sidebar > ul > li.active{background-color:#3a3a3a;background-image:-webkit-gradient(linear,0 0%,0 100%,from(#333333),to(#3F3F3F));background-image:-webkit-linear-gradient(top,#333333 0%,#3F3F3F 100%);background-image:-moz-linear-gradient(top,#333333 0%,#3F3F3F 100%);background-image:-ms-linear-gradient(top,#333333 0%,#3F3F3F 100%);background-image:-o-linear-gradient(top,#333333 0%,#3F3F3F 100%);background-image:linear-gradient(top,#333333 0%,#3F3F3F 100%)}
#sidebar > ul > li.open.submenu > a{border-bottom:1px solid #00223c}
#sidebar > ul > li.open.active.submenu > a{border-bottom:1px solid #2A2A2A}
#sidebar > ul > li > a > .label{background-color:#333333}
#sidebar > ul > li > a:hover{background-color:#084b7f; text-decoration:none;}
#sidebar > ul ul{border-top:1px solid #084b7f;background-color:#003557}
#sidebar > ul ul li a{border-top:1px solid #084b7f;border-bottom:1px solid #00223c}
#sidebar > ul ul li a:hover,#sidebar > ul ul li.active a{color:#CCCCCC;background-color:#084b7f; text-decoration:none;}
.dropdown-menu li a:hover,.dropdown-menu .active a,.dropdown-menu .active a:hover{color:#eeeeee;background-color:#444444;background-image:-webkit-gradient(linear,0 0%,0 100%,#555555,#222222);background-image:-webkit-linear-gradient(top,#555555 0%,#222222 100%);background-image:-moz-linear-gradient(top,#555555 0%,#222222 100%);background-image:-ms-linear-gradient(top,#555555 0%,#222222 100%);background-image:-o-linear-gradient(top,#555555 0%,#222222 100%);background-image:linear-gradient(top,#555555 0%,#222222 100%);filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#555555',endColorstr='#222222',GradientType=0 );
/* IE6-9 */}
#content{background:none repeat scroll 0 0 #eeeeee;margin-left:220px;height:100%;padding-bottom:25px;position:relative; min-height:100%; width:auto;border-top-left-radius:8px}

/*   2015-0310  改网页框架添加 css  end  */




